        function _refreshSessionTypeCookie(value) {
        if (value !== 'signup') {
                        value = _getSessionTypeCookie() === 'signup' ? 'signup' : 'active';
        }
        var d = new Date();
        d.setTime(d.getTime() + 1000 * 60 * 30);
        document.cookie = 'm_session_type=' + value + '; expires=' + d.toUTCString() + '; path=/; domain=' + '.mollie.com';
    }

    function _getSessionTypeCookie() {
        var match = document.cookie.match('m_session_type=([^;]*)');
        return match && match[1];
    }

    
            ga('send', 'pageview');
